{
  "files.exclude": {
    "out": true,
    "**/node_modules/**": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/.git": true,
  },
  "search.exclude": {
    "out": true,
    "**/node_modules/**": true,
    "**/*.code-search": true,
  },
  "chatcopycat.enableClipboardResetHotkey": false,
  "chatcopycat.inquiryType": false,
  "chatcopycat.showLanguageInSnippets": false,
  "chatcopycat.customDiagnosticsMessage": "- Please fix scripter typing for me plss :)"
}